<mvc:View controllerName="com.demo.ODataNavigationApplication.controller.View1" xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
	xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<List noDataText="Drop list items here" id="list0" items="{DataModel>/Products}">
				        <items>
				            <StandardListItem type="Navigation" title="{DataModel>ProductName}" id="item0" press="showDetails"/>
				        </items>
				    </List>

					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>